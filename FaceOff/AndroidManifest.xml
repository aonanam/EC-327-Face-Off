<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.faceoff"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="21" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/NoActionBar">
        <activity
            android:name="com.example.faceoff.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.faceoff.Tutorial"
            android:label="@string/title_activity_tutorial" >
        </activity>
        <activity
            android:name="com.example.faceoff.SinglePhone"
            android:label="@string/title_activity_single_phone" >
        </activity>
        <activity
            android:name="com.example.faceoff.CreateProfile"
            android:label="@string/title_activity_create_profile" >
        </activity>
        <activity
            android:name="com.example.faceoff.Player2"
            android:label="@string/title_activity_player2" >
        </activity>
        <activity
            android:name="com.example.faceoff.BaseFace"
            android:label="@string/title_activity_base_face" >
        </activity>
        <activity
            android:name="com.example.faceoff.BaseFaceTutorial"
            android:label="@string/title_activity_base_face_tutorial" >
        </activity>
        <activity
            android:name="com.example.faceoff.StartFaceOff"
            android:label="@string/title_activity_start_face_off" >
        </activity>
        <activity
            android:name="com.example.faceoff.Player1Offense"
            android:label="@string/title_activity_player1_offense" >
        </activity>
        <activity
            android:name="com.example.faceoff.Player2Defense"
            android:label="@string/title_activity_player2_defense" >
        </activity>
        <activity
            android:name="com.example.faceoff.Player2Offense"
            android:label="@string/title_activity_player2_offense" >
        </activity>
        <activity
            android:name="com.example.faceoff.Player1Defense"
            android:label="@string/title_activity_player1_defense" >
        </activity>
    </application>
</manifest>
